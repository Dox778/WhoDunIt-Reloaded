	Death.friendlymelee:
    Death.CueMelee:
    Death.ShovelMelee:
    Death.SawMelee:
    Death.friendlythrown:
    Death.fists:
		TNT1 A 0 A_Scream
		NULL AAA 0 A_SpawnItemEx ("Flaskshard1", 0, 0, random (3, 5), random (-5, 5), random (-5, 5), random (1, 2), 0, SXF_TRANSFERTRANSLATION|SXF_CLIENTSIDE, 30)
