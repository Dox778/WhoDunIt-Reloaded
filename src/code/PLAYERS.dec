actor DoDance1
{
	Radius 16
	Height 56
	Mass 2000
	-SOLID -SHOOTABLE
	+FLOORCLIP +NOBLOOD
	+NOGRAVITY +DONTSPLASH
	+FORCEYBILLBOARD +MOVEWITHSECTOR
	//+CLIENTSIDEONLY
	States
	{
	Spawn:
		JAKE A 0 NODELAY ACS_NamedExecute("WDISetDanceTID",0,0,0,0)
