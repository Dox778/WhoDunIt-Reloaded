Actor Innocent : PlayerPawn
{ 
	Radius 16
	Height 56
	PainChance 256
	Mass 2000
	Speed 0.8
	Alpha 1.0
	GibHealth -100
	Player.ViewHeight 50
	DeathHeight 8
	Player.ForwardMove 0.8 0.8
	Player.SideMove 0.4 0.4
	Player.JumpZ 8
	Player.ColorRange 112, 127
	Player.RunHealth 20
	Player.DisplayName "Voice: Thick"
	Player.SoundClass "TMC"
	
	//Player.StartItem "StartUpWeapon"
	Player.StartItem "NewFist"
	Player.StartItem "Action_Ping"
	Player.StartItem "SmallKit", 1
	Player.StartItem "Flare", 2 
	Player.StartItem "Warm", 1
	Player.StartItem HudAlive
	Player.StartItem MarkMe
	
	DamageFactor "Falling", 2.0
	DamageFactor "FlareEffect", 0.0
	//elemental damage
	Player.DamageScreenColor "Orange", 0.9, "Burn"
	Player.DamageScreenColor "Orange", 0.9, "Fire"
	Player.DamageScreenColor "Cyan", 0.9, "Drowning"
	Player.DamageScreenColor "LightBlue", 0.9, "Ice"
	Player.DamageScreenColor "Yellow", 0.9, "Electric"
	
	Player.DamageScreenColor "Blue", 0.9, "SprayBLU"
	Player.DamageScreenColor "Green", 0.9, "SprayGRN"
	Player.DamageScreenColor "Yellow", 0.9, "SprayYLW"
	Player.DamageScreenColor "Orange", 0.9, "SprayORG"
	Player.DamageScreenColor "Pink", 0.9, "SprayPNK"
	+NOSKIN +GHOST
	+WINDTHRUST +FORCEYBILLBOARD
	-TELESTOMP -NOBLOCKMONST
	+USEDAMAGEEVENTSCRIPT
	Species "Player"
	States
	{
	
	Spawn2:
		TNT1 A 0
	Spawn:
		PLAY A 90 A_AlertMonsters
        PLAY A 0 A_JumpIfInventory("Warm", 1, "Spawn")
		PLAY AAAAAAAAAAAAAAA 1 A_SpawnItemEx ("Breath", 8, 0, 46, random(0.5, 1.0), 0, random(-0.1, 0.1), random(-8.0, 8.0), 0, 0)
		TNT1 A 0 
		Loop
	See:
		TNT1 A 0 A_AlertMonsters
		TNT1 A 0 A_JumpIf(velx == 0 && vely == 0, "Spawn")
	TNT1 A 0 A_JumpIf(height < 40, "SeeCrouched")
		PLAY A 6 A_JumpIf(velx > 6 || vely > 6 || velx < -6 || vely < -6 , "SeeFast")
		PLAY B 6 
		PLAY C 6 A_SpawnItemEx("Footstep" ,0, -5, 12)
		PLAY D 6 
		NULL A 0 A_SpawnItemEx("Footstep" ,0, 5, 12)
		PLAY D 0 A_JumpIfInventory("Warm", 1, "Spawn2")
	TNT1 A 0 A_JumpIf(height < 40, "SeeCrouched")
		PLAY A 6 A_JumpIf(velx > 6 || vely > 6 || velx < -6 || vely < -6 , "SeeFast")
		PLAY B 6 
		PLAY C 6 A_SpawnItemEx("Footstep" ,0, -5, 12)
		PLAY D 6 
		NULL A 0 A_SpawnItemEx("Footstep" ,0, 5, 12)
	TNT1 A 0 A_JumpIf(height < 40, "SeeCrouched")
		PLAY A 6 A_JumpIf(velx > 6 || vely > 6 || velx < -6 || vely < -6 , "SeeFast")
		PLAY B 6 
		PLAY C 6 A_SpawnItemEx("Footstep" ,0, -5, 12)
		PLAY D 6 
		NULL A 0 A_SpawnItemEx("Footstep" ,0, 5, 12)
	TNT1 A 0 A_JumpIf(height < 40, "SeeCrouched")
		PLAY A 6 A_JumpIf(velx > 6 || vely > 6 || velx < -6 || vely < -6 , "SeeFast")
		PLAY B 6 
		PLAY C 6 A_SpawnItemEx("Footstep" ,0, -5, 12)
		PLAY D 6 
		NULL A 0 A_SpawnItemEx("Footstep" ,0, 5, 12)
	TNT1 A 0 A_JumpIf(height < 40, "SeeCrouched")
		PLAY A 6 A_JumpIf(velx > 6 || vely > 6 || velx < -6 || vely < -6 , "SeeFast")
		PLAY B 6 
		PLAY C 6 A_SpawnItemEx("Footstep" ,0, -5, 12)
		PLAY D 6 
		NULL A 0 A_SpawnItemEx("Footstep" ,0, 5, 12)
	TNT1 A 0 A_JumpIf(height < 40, "SeeCrouched")
		PLAY A 6 A_JumpIf(velx > 6 || vely > 6 || velx < -6 || vely < -6 , "SeeFast")
		PLAY B 6 
		PLAY C 6 A_SpawnItemEx("Footstep" ,0, -5, 12)
		PLAY D 6 
		NULL A 0 A_SpawnItemEx("Footstep" ,0, 5, 12)
	
	TNT1 A 0 A_JumpIf(height < 40, "SeeCrouched")
	TNT1 A 0 A_JumpIf(velx > 6 || vely > 6 || velx < -6 || vely < -6 , "SeeFast")
		PLAY AAAAAA 1 A_SpawnItemEx ("Breath", 8, 0, 48, random(0.5, 1.0), 0, random(-0.1, 0.1), random(-8.0, 8.0), 0, 0) //Lol all that for one breath. Hurr
		PLAY BBBBBB 1 A_SpawnItemEx ("Breath", 8, 0, 48, random(0.5, 1.0), 0, random(-0.1, 0.1), random(-8.0, 8.0), 0, 0)
		NULL A 0 A_SpawnItemEx("Footstep" ,0, -5, 12)
		PLAY CCCCCC 1 A_SpawnItemEx ("Breath", 8, 0, 48, random(0.5, 1.0), 0, random(-0.1, 0.1), random(-8.0, 8.0), 0, 0)
		PLAY D 6 
		NULL A 0 A_SpawnItemEx("Footstep" ,0, 5, 12)
		Goto Spawn		
	SeeCrouched:
		PLAY ABCD 5 
		Goto Spawn	
	
	SeeFast:
	TNT1 A 0 A_JumpIf(height < 40, "SeeCrouched")
	TNT1 A 0 A_JumpIf(velx == 0 && vely == 0, "Spawn")
	TNT1 A 0 A_JumpIf(velx > 6 || vely > 6 || velx < -6 || vely < -6 , 1)
	Goto Spawn
		PLAY A 4 
		PLAY B 4 
		PLAY C 4 A_SpawnItemEx("Footstep" ,0, -5, 12) 
		PLAY D 4 
	NULL A 0 A_SpawnItemEx("Footstep" ,0, 5, 12)
	PLAY D 0 A_JumpIfInventory("Warm", 1, "Spawn")
	TNT1 A 0 A_JumpIf(height < 40, "SeeCrouched")
	TNT1 A 0 A_JumpIf(velx == 0 && vely == 0, "Spawn")
	TNT1 A 0 A_JumpIf(velx > 6 || vely > 6 || velx < -6 || vely < -6 , 1)
	Goto Spawn
		PLAY AAAA 1 A_SpawnItemEx ("Breath", 8, 0, 48, random(0.5, 1.0), 0, random(-0.1, 0.1), random(-8.0, 8.0), 0, 0) //Lol all that for one breath. Hurr
		PLAY BBBB 1 A_SpawnItemEx ("Breath", 8, 0, 48, random(0.5, 1.0), 0, random(-0.1, 0.1), random(-8.0, 8.0), 0, 0)
		NULL A 0 A_SpawnItemEx("Footstep" ,0, -5, 12) 
		PLAY CCCC 1 A_SpawnItemEx ("Breath", 8, 0, 48, random(0.5, 1.0), 0, random(-0.1, 0.1), random(-8.0, 8.0), 0, 0)
		PLAY D 4 
		NULL A 0 A_SpawnItemEx("Footstep" ,0, 5, 12)
		Goto Spawn		
		
	PlayDead:
		TNT1 A 0 //SetPlayerProperty(0,1,PROP_TOTALLYFROZEN )
		TNT1 A 0 //A_ChangeFlag("NOPAIN",1)
		TNT1 A 0 //A_ChangeFlag("FORCEXYBILLBOARD",1)
		PLAY H 9 //A_NoBlocking
		PLAY I 9 //A_Pain
		PLAY JK 9 
		PLAY LMN 3
		TNT1 A 0 //SetPlayerProperty(0,0,PROP_TOTALLYFROZEN )
		TNT1 A 0 //SetPlayerProperty(0,1,PROP_FROZEN )
	StayDown:
		PLAY N 4
		Loop
		PLAY AAA 0
	GetUp:
		PLAY L 5
		PLAY K 5 
		PLAY JI 5
		PLAY I 4 //A_ChangeFlag("FORCEXYBILLBOARD",0)
		PLAY H 3 
		//NULL A 0 A_ChangeFlag("NOPAIN",0)
		//NULL A 0 SetPlayerProperty(0,0,PROP_FROZEN )
		Goto Spawn
	
	DoDance1:
		TNT1 A 0 A_SpawnItemEx("DoDance1", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION | SXF_TRANSFERTRANSLATION | SXF_TRANSFERSCALE  | SXF_SETMASTER , 0, tid)
		Goto DoDance
	DoDance2:
		TNT1 A 0 A_SpawnItemEx("DoDance2", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION | SXF_TRANSFERTRANSLATION | SXF_TRANSFERSCALE  | SXF_SETMASTER , 0, tid)
		Goto DoDance
	DoDance3:
		TNT1 A 0 A_SpawnItemEx("DoDance3", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION | SXF_TRANSFERTRANSLATION | SXF_TRANSFERSCALE  | SXF_SETMASTER , 0, tid)
		Goto DoDance
	DoDance4:
		TNT1 A 0 A_SpawnItemEx("DoDance4", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION | SXF_TRANSFERTRANSLATION | SXF_TRANSFERSCALE  | SXF_SETMASTER , 0, tid)
		Goto DoDance
	DoDance5:
		TNT1 A 0 A_SpawnItemEx("DoDance5", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION | SXF_TRANSFERTRANSLATION | SXF_TRANSFERSCALE  | SXF_SETMASTER , 0, tid)
		Goto DoDance
	DoDance6:
		TNT1 A 0 A_SpawnItemEx("DoDance6", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION | SXF_TRANSFERTRANSLATION | SXF_TRANSFERSCALE  | SXF_SETMASTER , 0, tid)
	DoDance:
		TNT1 A 104 A_ChangeFlag("NOPAIN",1)
		TNT1 A 0 A_ChangeFlag("NOPAIN",0)
		TNT1 A 0 A_TakeInventory("IsDancing",1)
		goto Spawn	
	
	Missile:
		PLAY "#" 0 A_JumpIfInventory("Pump0",1,"Pump0")
		PLAY "#" 0 A_JumpIfInventory("Pump1",1,"Pump1")
		PLAY "#" 0 A_JumpIfInventory("Pump2",1,"Pump2")
		PLAY E 6 A_AlertMonsters
		PLAY A 0 A_JumpIfInventory("PlayerAttackAnim",1,"Missile2")
		PLAY A 0 A_JumpIfInventory("NeedPump",1,"Pump")
		NULL A 0 SetPlayerProperty(0,0,PROP_TOTALLYFROZEN )
		Goto See
	Missile2:
		PLAY E 2
		PLAY F 5
		TNT1 A 0 A_TakeInventory("PlayerAttackAnim",1)
		Goto See
	Pump:
		PLAY E 3
		PLAY J 5
		PLAY KJ 7 A_TakeInventory("NeedPump",1)
		Goto See
	Pump0:
		PLAY J 8
	Pump1:
		PLAY K 2
	//	Goto See
	Pump2:
		PLAY J 8
		Goto See
	Melee:
		TNT1 A 0 A_JumpIfInventory("IsPunching",1,"Missile")
		PLAY F 4 Bright A_AlertMonsters
		Goto Missile
	MurdKick:
		PLAY CD 4
		PLAY X 10
//		PLAY DC 4
		Goto Spawn
		
	
	Pain:
	Pain.Default: 
		TNT1 A 0 ACS_NamedExecuteAlways("OuchCamera",0)
		TNT1 A 0 A_GiveInventory ("GotHurt", 6)
		TNT1 A 0 ACS_NamedExecuteAlways("NewScreenBlood",0)
		PLAY G 4 A_Quake(2,8,0,6,"none")
		PLAY G 4 A_Pain
		PLAY G 0 A_JumpIfInventory("Warm", 1, "Spawn")
		PLAY GGGGGGGGGG 1 A_SpawnItemEx ("Breath", 8, 0, 48, random(0.5, 1.0), 0, random(-0.1, 0.1), random(-8.0, 8.0), 0, 0)
		Goto Spawn
		
	Pain.Kick:
		TNT1 A 0 ACS_NamedExecuteAlways("OuchCamera",0)
		TNT1 A 0 A_GiveInventory ("GotHurt", 6)
		NULL A 0 A_GiveInventory ("SlowCounter", 5)
		PLAY G 4 A_Quake(2,8,0,6,"none")
		PLAY G 4 A_Pain
		PLAY G 0 A_JumpIfInventory("Warm", 1, "Spawn")
		PLAY GGGGGGGGGG 1 A_SpawnItemEx ("Breath", 8, 0, 48, random(0.5, 1.0), 0, random(-0.1, 0.1), random(-8.0, 8.0), 0, 0)
		Goto Spawn
		
	Pain.Grab:
		NULL A 0 ACS_NamedExecuteAlways("OuchCamera",0)
		NULL A 0 A_GiveInventory ("GotHurt", 6)
		NULL A 0 A_GiveInventory ("HasGrabbed", 1)
		
		PLAY A 0 SetPlayerProperty(0,1,PROP_TOTALLYFROZEN )
		NULL G 0 A_Stop
		PLAY G 2 A_Pain
		PLAY G 6 ThrustThingZ(0,13,0,1)
		PLAY G 6 ThrustThingZ(0,11,0,1)
		NULL A 0 SetPlayerProperty(0,0,PROP_TOTALLYFROZEN )
		NULL A 0 A_TakeInventory ("HasGrabbed", 1)
		PLAY G 5 ThrustThingZ(0,15,0,1)
		Goto Spawn
	GrabbingPlayer:
		PLAY A 0 A_ChangeFlag("NOPAIN",1)
		PLAY F 14 A_stop
		PLAY A 0 A_ChangeFlag("NOPAIN",0)
		Goto Spawn
		
	Pain.ShotgunTrap:		
		TNT1 A 0 ACS_NamedExecuteAlways("OuchCamera",0)
		TNT1 A 0 A_GiveInventory ("GotHurt", 6)
		TNT1 A 0 ACS_NamedExecuteAlways("NewScreenBloodNoLimit",0)
		TNT1 A 0 ACS_NamedExecuteAlways("NewScreenBullet",0)
		PLAY G 4 A_Quake(2,8,0,6,"none")
		PLAY G 4 A_Pain
		PLAY G 0 A_JumpIfInventory("Warm", 1, "Spawn")
		PLAY GGGGGGGGGG 1 A_SpawnItemEx ("Breath", 8, 0, 48, random(0.5, 1.0), 0, random(-0.1, 0.1), random(-8.0, 8.0), 0, 0)
		Goto Spawn
	Pain.Knife:		
		TNT1 A 0 ACS_NamedExecuteAlways("OuchCamera",0)
		TNT1 A 0 A_GiveInventory ("GotHurt", 6)
		TNT1 A 0 ACS_NamedExecuteAlways("NewScreenBlood",0)
		PLAY G 4 A_Quake(2,8,0,6,"none")
