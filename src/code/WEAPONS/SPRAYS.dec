Actor SprayCanBLU : Wdi_BaseWeapon
{
	Damage (1)
	Scale 0.5
	Tag "Blue Spray Can"
	Inventory.PickupMessage "Found a Spray can."
	Inventory.PickupSound "Misc/I_pkup"
	Weapon.AmmoType1 "SprayAmmo"
	Weapon.AmmoUse 1
	Weapon.PreferredSkin "SprayCanSkin"
	Weapon.UpSound "DSSPRYSH"
	+WEAPON.WIMPY_WEAPON
	+WEAPON.AMMO_OPTIONAL
	+DonTGib
	States
	{
	Spawn:
		SPRY B -1
		Stop
	Ready:
		
		SPRY A 0 A_StopSoundEx("Body")
		SPRY Y 1 A_WeaponReady(WRF_ALLOWZOOM)
		Loop
	Select:
		SPRY YYYYYYYYYYYYYYY 0 A_Raise
		"####" "#" 1 offset(36,76)
		"####" "#" 1 offset(28,64)
		"####" "#" 1 offset(20,54)
		"####" "#" 1 offset(14,45)
		"####" "#" 1 offset(8,40)
		"####" "#" 1 offset(4,36)
		"####" "#" 1 offset(1,33)
		goto Ready	
	FireEmpty:
		SPRY Z 1 A_StopSoundEx("Body")
		SPRY A 0 A_ReFire
		Goto Ready
	Fire:
		TNT1 A 0 A_JumpIfInventory("SprayAmmo",1,2)
		Goto FireEmpty
		TNT1 AAA 0
		SPRY A 0 A_PlaySoundEx("DSSPRY", "Body", 1)
		SPRY Z 1 Offset (0,34) A_FireCustomMissile ("PaintSprayBLU",0,1)
		SPRY Z 1 Offset (0,33) A_FireCustomMissile ("PuffBLU",0,0)
		
		SPRY A 0 A_ReFire
		SPRY A 0 A_StopSoundEx("Body")
		Goto Ready
	Death:
		BBAT A 0 A_SpawnItem("DroppedSprayCanBLU",0,0,0,0)
		TNT1 A 0 A_FadeOut(1.0)
		Goto Death + 1
	//Zoom:
		TNT1 A 0
		TNT1 A 0 A_TakeInventory("SprayCanOnce",1)
	  NULL A 0 A_GiveInventory("DropAnimWeapon",1)
	  NULL A 0 A_SelectWeapon("DropAnimWeapon")
		TNT1 A 0 A_TakeInventory("IsDropping",1)
		TNT1 A 0 ACS_ExecuteAlways(418,0,0,0,0)
		Goto Ready
}}

