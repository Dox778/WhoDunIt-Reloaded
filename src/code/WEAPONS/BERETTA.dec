Actor FakeBeretta : CustomInventory Replaces Pistol //5010
{
	//$Category WDI Weapons/Limited Weapons
	//$Sprite PISTA0
	//$Title "Beretta"
	Radius 16
	Height 8
	Inventory.PickupMessage "\cjFound an \c[R5]M1911 Colt. \cjDon't fire it too fast! \n\caDamage: \c[C4]16"
	Inventory.Respawntics 42000 //20min
	Scale 0.9
	+FORCEXYBILLBOARD
	+DonTGib
	States
	{
	Spawn:
		TNT1 A 0 NODELAY A_Jump(128,2)
		TNT1 A 0 A_SetScale(-scalex,scaley)
		PIST A -1
		Stop
	Pickup:
		TNT1 A 0
		TNT1 A 0 A_JumpIfInventory("MurderMarker",1, 2)
		TNT1 A 0 A_JumpIfInventory("TKToken",1,"NotPickup")
		TNT1 A 0
		TNT1 A 0 A_JumpIfInventory("Beretta",1,"AmmoPickup")
		TNT1 A 0 A_JumpIfInventory("WDIBeretta",1,"AmmoPickup")
		TNT1 A 0 A_JumpIfInventory("Slot1Filled",1,"NotPickup")
		TNT1 A 0 A_PlaySound("Pickups/Weapon")
		NULL A 0 A_JumpIfinventory("MurderMarker",1,"PickupMurd")
		TNT1 A 0 A_GiveInventory("Beretta",1)
	ContinuePickup:
		TNT1 A 0 A_GiveInventory("Slot1Filled",1)	 TNT1 A 0 A_GiveInventory("GotNewWeap",1) 
		TNT1 A 0 A_GiveInventory("PistolAmmo",Random(0,10))
		TNT1 A 1 ACS_NamedExecute("OldWeapon_ID",0,3)
		Stop
	PickupMurd:
		TNT1 A 0 A_GiveInventory("MurdBeretta",1)
		TNT1 A 0 A_Log("\cjMurderer's colt is modified in multiple ways...")
		TNT1 A 0 A_Jump(256,"ContinuePickup")
		Goto ContinuePickup
	NotPickup:
		TNT1 A 0
		TNT1 A 1 ACS_NamedExecute("OldWeapon_ID",0,3)
		Fail
	AmmoPickup:
		TNT1 A 0
		TNT1 A 0 A_JumpIfInventory("PistolAmmo",10,"AmmoPickupFull")
		TNT1 A 0 A_PlaySound("Pickup/Ammo")
		TNT1 A 0 A_GiveInventory("PistolAmmo",Random(0,10))
		TNT1 A 0 A_FireCustomMissile("DroppedBerettaEmpty",0,0,0)
		TNT1 A 1 ACS_NamedExecute("OldWeapon_ID",0,3)
		Stop
	AmmoPickupFull:
		TNT1 A 0
		TNT1 A 0 A_JumpIfInventory("PistolClip",2,"ForgetIt")
		TNT1 A 0 A_PlaySound("Pickup/Ammo")
		TNT1 A 0 A_GiveInventory("PistolClip",1)
		TNT1 A 0 A_FireCustomMissile("DroppedBerettaEmpty",0,0,0)
		TNT1 A 1 ACS_NamedExecute("OldWeapon_ID",0,3)
		Stop
	ForgetIt:
		TNT1 A 1
		Stop
}}
