//M1911 
Actor Beretta : Wdi_BaseWeapon
{ 
	Tag "M1911 Colt"
	Weapon.PreferredSkin "PistolSkin"
	Weapon.SelectionOrder 400
	Weapon.SlotNumber 1
	Weapon.Kickback 100
	Weapon.AmmoType "PistolAmmo"
	Weapon.AmmoType2 "PistolClip"
	Weapon.AmmoUse 0
	-WEAPON.MELEEWEAPON
	States
	{
	Spawn:
		PIST A -1
		Stop
	Select:
		TNT1 A 5 A_TakeInventory("FidgetSpam",1)
		REPS AAAAAAAAAAAAAAA 0 A_Raise
		REPS P 0 A_JumpIfInventory("NeedToUnjamPistol",1,"SelectJammed")
		Goto SelectAnim
		
	SelectAnim:
		"####" P 1 offset(71,156) A_PlaySound("PISTJMSL",CHAN_WEAPON)
		"####" P 1 offset(62,127)
		"####" P 1 offset(52,106)
		"####" O 1 offset(44,88)
		"####" N 1 offset(36,76) A_PlaySound("PISTJMFX",CHAN_WEAPON)
		"####" M 1 offset(28,64)
		"####" L 1 offset(20,54)
		"####" K 1 offset(14,45)
		"####" J 1 offset(8,40)
		"####" I 1 offset(4,36)
		"####" H 1 offset(1,33)
		"####" GF 1 A_WeaponReady(WRF_NOBOB|WRF_NOFIRE)
		"####" "#" 0 A_Jump(255,"Ready")
		"####" "#" 0 A_Jump(255,"Ready")
		goto Ready	
		
	SelectJammed:
		REPS K 1 A_GiveInventory("JamCountPistol2",3)
		Goto SelectAnim
		
	Ready:
		TNT1 A 0
		TNT1 A 0 A_JumpIfInventory("NeedToUnjamPistol",1,"ReadyJammed")
		TNT1 A 0 A_TakeInventory("JamCountPistol2",1)
		 REPS A 1 A_WeaponReady(WRF_ALLOWZOOM|WRF_ALLOWRELOAD)
		 REPS A 1 A_WeaponReady(WRF_ALLOWZOOM|WRF_ALLOWRELOAD)
		 REPS A 1 A_WeaponReady(WRF_ALLOWZOOM|WRF_ALLOWRELOAD)
		 REPS A 1 A_WeaponReady(WRF_ALLOWZOOM|WRF_ALLOWRELOAD)
		 REPS A 1 A_WeaponReady(WRF_ALLOWZOOM|WRF_ALLOWRELOAD)
		 REPS A 1 A_WeaponReady(WRF_ALLOWZOOM|WRF_ALLOWRELOAD)
		 REPS A 1 A_WeaponReady(WRF_ALLOWZOOM|WRF_ALLOWRELOAD)
		 REPS A 1 A_WeaponReady(WRF_ALLOWZOOM|WRF_ALLOWRELOAD)
		 REPS A 1 A_WeaponReady(WRF_ALLOWZOOM|WRF_ALLOWRELOAD)
		 REPS A 1 A_WeaponReady(WRF_ALLOWZOOM|WRF_ALLOWRELOAD)
		 REPS A 1 A_WeaponReady(WRF_ALLOWZOOM|WRF_ALLOWRELOAD)
		 REPS A 1 A_WeaponReady(WRF_ALLOWZOOM|WRF_ALLOWRELOAD)
		loop 
	ReadyJammed:
		REPS K 1 A_WeaponReady(WRF_ALLOWZOOM|WRF_NOSECONDARY|WRF_NOSWITCH)
		loop 
	
	Fire:
		REPS A 0
		REPS A 0 A_JumpIfInventory("NeedToUnjamPistol",1,"TryToUnjam")
		REPS A 0 A_JumpIfInventory("PistolAmmo",1,2)
		Goto NoAmmo
		REPS AAA 0
		NULL A 0 A_JumpIfInventory("JamCountPistol2",3,"Jam")//forces the gun to jam if you fire too fast a few times
		REPS A 0 ACS_NamedExecuteAlways("Powercounter",0)
		M1TU A 0 A_GiveInventory("Powercounter",1)//< speed trigger
		M1TU A 0 A_JumpIfInventory("M1AntiJam",1,"Fakefire") //Goodbye Jambingo D:
		M1TU A 0 A_JumpIfInventory("Powercounter",3,"JAM60") //trigger this at 60%
		M1TU A 0 A_JumpIfInventory("Powercounter",2,"JAM40") //40%
	Fakefire:
		BDPI B 0 Bright A_TakeInventory("PistolAmmo",1)
		2EPR AA 0 A_FireCustomMissile ("PistolSmokeSpawner", 0, 0, 4,12, 0, 0)
		RIFF A 0 A_FireCustomMissile("BulletTracer", frandom(1.8,2.8), 0, 9, 7)
		PISG B 0 A_FireBullets(1.50, 1.0, -1, 16, "FFBulletPuff",FBF_NORANDOM)
		PISG B 0 A_PlaySound("PistolV2/Fire",CHAN_WEAPON)
		PISG B 0 A_AlertMonsters
		TNT1 A 0 A_GunFlash("RecoilStuff",GFF_NOEXTCHANGE)
		REPS B 1 Bright A_SpawnItem("FMBulletPuff2Lighter",10,30)
		PISG B 0 A_FireCustomMissile ("PisCaseSpawn", 0, 0, 3, 10, 0, 0)
		NULL A 0 A_GiveInventory("JamCountPistol2",1)//shots counter
		REPS CDE 1 A_TakeInventory("M1AntiJam")
		REPS FF 1 A_WeaponReady(WRF_NOBOB|WRF_NOSWITCH)
		REPS A 5 A_WeaponReady(WRF_NOBOB|WRF_NOSWITCH)
		Goto Ready
	RecoilStuff:
		NULL A 1 A_SetPitch(Pitch-1)
		NULL A 0 A_SetPitch(Pitch-3)
		TNT1 A 0 A_SetAngle(random(-2,2)+angle)
		TNT1 A 1 A_ZoomFactor(0.99)
		TNT1 A 0 A_ZoomFactor(1.0)
		TNT1 AA 1 A_SetPitch(Pitch+1)
		TNT1 A 1 A_SetPitch(Pitch+0.5)
		stop
		
	Jam40:
		M1TU A 0 A_Jump(6,"Jam")
		Goto Fakefire
	Jam60:
		M1TU A 0 A_Jump(10,"Jam")
		Goto Fakefire		
		
	Jam:
		2EPR A 0 
		NULL A 0 A_TakeInventory("Powercounter",10)
		TNT1 A 0 A_TakeInventory("JamCountPistol",10)
		TNT1 A 0 A_GiveInventory("JamCountPistol2",3)
		REPS A 2 offset(0,34) A_PlaySound("pistol/DRYFIRE")
		REPS A 1 offset(0,33)
		REPS A 6 offset(0,32)
		REPS A 1 A_Print("Your pistol is jammed.",2)
		REPS A 2 offset(0,34) A_PlaySound("pistol/DRYFIRE")
		REPS A 1 offset(0,33)
		REPS A 4 offset(0,32)
		REPS GHIJK 1
		TNT1 A 0 A_GiveInventory("NeedToUnjamPistol",1)
		Goto Ready
	TryToUnjam:	
		TNT1 A 0 A_Jump(12,"UnjamFinish")
		TNT1 A 0 A_JumpIfInventory("JamCountPistol",10,"UnjamFinish")
		REPS M 1 A_PlaySound("PISTJMSL",CHAN_WEAPON)
		REPS M 2
		REPS L 1 A_GiveInventory("JamCountPistol",1)
		Goto ReadyJammed
	UnjamFinish:
		NULL A 0
		REPS L 1 A_PlaySound("PISTJMSL",CHAN_WEAPON)
		REPS NO 1
		REPS P 1 A_PlaySound("PISTJMFX",CHAN_WEAPON)
		TNT1 A 0 A_TakeInventory("JamCountPistol2",3)
		REPS QRSSTTTUVWX 1
		NULL A 0 A_TakeInventory("NeedToUnjamPistol",10)
		NULL A 0 A_TakeInventory("JamCountPistol",10)
		NULL A 0 A_GiveInventory("M1AntiJam")
		Goto Ready	
		/*
	NoAmmo:
		2EPR A 0 A_PlaySound("Newpistol/DRYFIRE")
		REPS A 2 offset(0,34)
		REPS A 1 offset(0,33)
		REPS A 1 A_WeaponReady(WRF_NOFIRE)
		Goto Ready
		*/
	NoAmmo:
		REPS A 0 A_PlaySound("DRYF1", CHAN_WEAPON)
	NoAmmoHold:
		REPS A 1 offset(0,34)
		REPS A 1 A_ReFire("NoAmmoHold")
		"####" "#" 0 A_PlaySound("DRYF2", CHAN_WEAPON)
		"####" "#" 1 A_WeaponReady(WRF_NOFIRE)
		Goto Ready	
		
	ToReady:
		2EPR A 0
		Goto Ready
	AltFire:
		BDRI A 0 A_JumpIfInventory("PistolAmmo",10,"Reload")
		BDRI A 0 A_JumpIfInventory("PistolClip",1,"ReloadB")
		Goto Reload
	
	Reload:
		REPS GHIJ 1 A_WeaponReady(WRF_NOSECONDARY)
		REPS KKKK 1 A_WeaponReady(WRF_NOSECONDARY)
		REPS L 0 A_PlaySound("PISTJMSL",CHAN_WEAPON)
		REPS MNO 1 A_WeaponReady(WRF_NOSECONDARY)
	FidgetHold:
		REPS P 1 A_WeaponReady(WRF_NOSECONDARY)
		NULL A 0 A_ReFire("FidgetHold")
		REPS Q 0 A_PlaySound("PISTJMFX",CHAN_WEAPON)
		REPS PQRSSTTTUVWX 1 A_WeaponReady(WRF_NOSECONDARY)
		Goto ToReady
		
	ReloadB:
		REPR ABCC 1 A_WeaponReady(WRF_NOFIRE)
		TNT1 A 0 A_PlaySound("pistolV2/Clipout")
		REPR D 1 A_WeaponReady(WRF_NOFIRE)
		M1TR E 0 A_FireCustomMissile("PistolMagEmpty",random(30,40),0,0,0)
		REPR EFFFFFGGH 1 A_WeaponReady(WRF_NOFIRE)
		NULL A 0 A_PlaySound("pistolV2/Clipin") 
		REPR IJJ 1 A_WeaponReady(WRF_NOFIRE)
		REPR K 7 A_WeaponReady(WRF_NOFIRE)
		REPR LMNOP 1 A_WeaponReady(WRF_NOFIRE)
		M1GN A 0 A_JumpIfInventory("PistolClip",1,2)
        goto CanIhavesex
        TNT1 A 0
		TNT1 A 0 A_TakeInventory("PistolClip",1)
		TNT1 a 0 A_JumpIfInventory("PistolAmmo",1,2)
		Goto ReloadEmpty
		TNT1 AAA 0
		REPR O 0 A_GiveInventory("PistolAmmo",10)
		REPR Q 4
		REPR RS 1
		REPR T 2
		REPR UUVWXYY 1
		REPS A 1 A_TakeInventory("JamCountPistol",10)
		Goto Ready
		
	ReloadEmpty:
		REPS O 0 A_GiveInventory("PistolAmmo",10)
		REPR Q 4
		REPR RS 1
		REPR T 2
		REPR UUVWXYY 1
		REPS G 1 A_TakeInventory("JamCountPistol",10)
		REPS HIJ 1
		REPS KK 1
		REPS L 0 A_PlaySound("PISTJMSL",CHAN_WEAPON)
		REPS MNOPPP 1
		REPS Q 0 A_PlaySound("PISTJMFX",CHAN_WEAPON)
		REPS PQRSSTTTUVWX 1
		Goto ToReady
	canIhavesex:
        tnt1 a 0 a_print("fuck you now its jammed")
        REPR Q 4
		REPR RS 1
		REPR T 2
		REPR UUVWXYY 1
		REPS A 1 A_takeInventory("JamCountPistol",10)
		REPS A 1 A_giveInventory("NeedToUnjamPistol",10)
		Goto Ready
	//zoom:
		"####" "#" 0
		"####" "#" 1 offset(3,34)
		"####" "#" 1 offset(7,39)
		"####" "#" 1 offset(12,46)
		"####" "#" 1 offset(20,57)
		"####" "#" 1 offset(32,70)
		
		TNT1 A 0 A_TakeInventory("PistolOnce",1)
		TNT1 A 0 A_TakeInventory("IsDropping",1)
	  NULL A 0 A_GiveInventory("DropAnimWeapon",1)
	  NULL A 0 A_SelectWeapon("DropAnimWeapon")
		TNT1 A 0 ACS_ExecuteAlways(418,0,0,0,0)
		Goto Ready
}}

