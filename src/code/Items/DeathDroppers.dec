//DEATH ITEM DROPS

ACTOR SmallKitPlayerDrop
{
	Radius 1
	Height 2
	Renderstyle none
	+THRUACTORS -SOLID
	States
	{
	Spawn:
		TNT1 A 1 NODELAY A_SpawnItemEx("SmallKit",random(-4,4),random(-4,4),random(0,8),   random(-2,2),random(-2,2),random(2,8),random(0,90),SXF_NOCHECKPOSITION,0)
		stop
	}
}
