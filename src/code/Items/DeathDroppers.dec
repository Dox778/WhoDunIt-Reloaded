
Actor DeathDropper : CustomInventory
{
 Inventory.Amount 1
 Inventory.MaxAmount 0
 +INVENTORY.AUTOACTIVATE
 +INVENTORY.ALWAYSPICKUP
 +INVENTORY.UNDROPPABLE
 -INVENTORY.INVBAR
 +FORCEXYBILLBOARD
 States
 {
   Spawn:
	TNT1 A 1
	Wait

   Use:
	TNT1 A 1 
	Stop
 }
}


